omId = Math.trunc(Math.random() * 2000);\n    setImageUrl(`https://picsum.photos/id/${randomId}/200/300`);\n    console.log(imageUrl);\n  };\n\n  return (\n    <form onSubmit={handleSubmit(onHandleSubmit)}>\n      <div>\n        <Label for=\"titleId\">Title</Label>\n        <input\n          {...register(\"title\")}\n          value={isAddMode ? initalValue.title : editedPhoto.title}\n          placeholder=\"Eg: Wow nature ... \"\n        />\n      </div>\n\n      <div>\n        <Label for=\"categoryId\">Category</Label>\n        <select\n          value={isAddMode ? initalValue.category : editedPhoto.category}\n          id=\"categoryId\"\n          name=\"categoryId\"\n          {...register(\"category\")}\n        >\n          {PHOTO_CATEGORY_OPTIONS.map((item) => (\n            <option key={item.id}>{item.label}</option>\n          ))}\n        </select>\n      </div>\n      <div>\n        <Label for=\"categoryId\">Photo</Label>\n        <button type=\"button\" onClick={getRandomImageUrl}>\n          Random a Photo\n        </button>\n      </div>\n\n      <div>\n        <Controller\n          control={control}\n          name=\"photo\"\n          {...register(\"photo\")}\n          render={({ field }) => (\n            <RandomPhoto\n              {...register(\"id\")}\n              imagesUrl={imageUrl}\n            ></RandomPhoto>\n          )}\n        ></Controller>\n      </div>\n      <div>\n        <Button type=\"submit\" color=\"primary\">\n          Add to album\n        </Button>\n      </div>\n    </form>\n  );\n}\nexport default PhotoForm;\n","/Users/nhly1204/Documents/LEARNJS/ReactJs/PhotoApp/src/constants/global.js",[],[],"/Users/nhly1204/Documents/LEARNJS/ReactJs/PhotoApp/src/components/RandomPhoto/index.jsx",["107","108"],[],"import { useState } from \"react\";\nimport { Button } from \"reactstrap\";\nimport \"./RandomPhoto.scss\";\n\nfunction RandomPhoto(props) {\n  return (\n    <div className=\"random-photo\">\n      <div value={props.imagesUr